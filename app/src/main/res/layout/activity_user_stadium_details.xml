<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserStadiumDetails">

    <TextView
        android:id="@+id/txtTitle"
        android:layout_width="421dp"
        android:layout_height="31dp"
        android:background="#11224E"
        android:text="@string/titleStadium"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txtNameOfSt"
        android:layout_width="123dp"
        android:layout_height="26dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="268dp"
        android:fontFamily="sans-serif-medium"
        android:text="TextView"
        android:textColor="#0C0C0C"
        android:textSize="18sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtStadiumInfo"
        android:layout_width="273dp"
        android:layout_height="80dp"
        android:layout_marginTop="272dp"
        android:layout_marginEnd="68dp"
        android:background="#7095F6"
        android:backgroundTint="#B5C6F4"
        android:outlineProvider="paddedBounds"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtSeats"
        android:layout_width="85dp"
        android:layout_height="25dp"
        android:layout_marginStart="36dp"
        android:layout_marginBottom="212dp"
        android:background="#B5C6F4"
        android:backgroundTint="#B5C6F4"
        android:text="@string/MsgSeat"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtSize"
        android:layout_width="80dp"
        android:layout_height="26dp"
        android:layout_marginEnd="32dp"
        android:background="#B5C6F4"
        android:backgroundTint="#B5C6F4"
        android:text="@string/Msgsize"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textStyle="italic"
        app:layout_constraintBaseline_toBaselineOf="@+id/txtSeats"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/txtSeatValue"
        android:layout_width="64dp"
        android:layout_height="38dp"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="148dp"
        android:background="#B5C6F4"
        android:backgroundTint="#B5C6F4"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtSizeValue"
        android:layout_width="62dp"
        android:layout_height="33dp"
        android:layout_marginEnd="40dp"
        android:background="#B5C6F4"
        android:backgroundTint="#B5C6F4"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/txtSeatValue"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/btnMatchinStadium"
        android:layout_width="208dp"
        android:layout_height="40dp"
        android:layout_marginEnd="88dp"
        android:layout_marginBottom="32dp"
        android:backgroundTint="#32379A"
        android:text="@string/msgMa"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/txtCountry"
        android:layout_width="123dp"
        android:layout_height="24dp"
        android:layout_marginTop="144dp"
        android:layout_marginEnd="40dp"
        android:background="#B5C6F4"
        android:backgroundTint="#B5C6F4"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="#090909"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtLocation"
        android:layout_width="113dp"
        android:layout_height="24dp"
        android:layout_marginEnd="248dp"
        android:background="#B5C6F4"
        android:backgroundTint="#B5C6F4"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="#121111"
        android:textStyle="italic"
        app:layout_constraintBaseline_toBaselineOf="@+id/txtCountry"
        app:layout_constraintEnd_toEndOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>